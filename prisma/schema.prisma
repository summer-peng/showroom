// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  firstName String
  lastName  String
  email     String  @unique
  password  String?
  userId    Int?    @unique
}

model resumes {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  email      String
  title      String
  summary    String
  reamrk     String
  createDate DateTime
  updateDate DateTime
}
